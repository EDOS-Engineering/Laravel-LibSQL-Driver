{
    "name": "nick-potts/laravel-libsql-driver",
    "description": "Laravel integration for Turso LibSql services.",
    "keywords": [
        "laravel",
        "php",
        "database",
        "query",
        "sqlite",
        "turso",
        "libsql"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/nick-potts/laravel-libsql",
    "authors": [
        {
            "name": "Nick Potts",
            "homepage": "https://github.com/nick-potts",
            "role": "Developer"
        }
    ],
    "require": {
        "doctrine/dbal": "^3.6",
        "illuminate/cache": "^10.32|^11.0",
        "illuminate/database": "^10.32|^11.0",
        "saloonphp/laravel-http-sender": "^v3.0.0",
        "saloonphp/laravel-plugin": "^3.0.0",
        "saloonphp/saloon": "^3.0.0"
    },
    "suggest": {
        "ext-pdo": "Required to extend the PDO driver for LibSql."
    },
    "autoload": {
        "psr-4": {
            "NickPotts\\LibSql\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NickPotts\\LibSql\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.3",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^9.6|^10.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "LibSqlServiceProvider"
            ]
        }
    }
}
